<nav class="d-flex justify-content-center">
    <ul class="pagination">
        {% for page in 1 .. max([1,pagination.maxPage]) %}
            <li class="page-item">
                <a class="page-link {{ this.currentPage == page ? 'active' : null }}" href="{{ this.pageUrl(page) }}">{{ page }}</a>
            </li>
        {% endfor %}
    </ul>
</nav>
